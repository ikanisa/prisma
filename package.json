  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host --port 8080",
    "check:bundle": "node ./scripts/check_bundlesize.mjs",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:workspace": "pnpm -r --if-present run lint",
    "config:validate": "node scripts/config/validate-config.mjs",
    "codegen:aat": "openapi-typescript openapi/aat_agent_api.yaml -o src/integrations/aat-agent/types.ts",
    "test": "vitest run",
    "test:workspace": "pnpm -r --if-present run test",
    "coverage": "./scripts/coverage.sh",
    "build:workspace": "pnpm -r --if-present run build",
    "ci:verify": "pnpm run lint:workspace && pnpm run test:workspace && pnpm run build:workspace",
    "test:playwright": "playwright test",
    "test:playwright:core": "playwright test tests/playwright/core-journeys.spec.ts",
    "agents:generate": "ts-node scripts/generate_agent_manifests.ts",
    "agents:publish": "ts-node scripts/publish_agent_manifests.ts",
    "agents:update": "ts-node scripts/update_agent_manifest_ids.ts",
    "agents:publish:dry": "npm run agents:generate && npm run agents:publish -- --dry-run",
    "agents:publish:staging": "npm run agents:generate && npm run agents:publish && npm run agents:update",
    "agents:evaluate": "node scripts/run_agent_evaluations.js",
    "openai:file-search:secrets": "ts-node scripts/operations/publish-openai-file-search-secrets.ts",
    "load:test": "pnpm exec artillery run tests/load/artillery/core-journeys.yml",
    "load:test:ci": "pnpm exec artillery run tests/load/artillery/core-journeys.yml --dry-run",
    "catalog:check": "node ./scripts/check-service-ownership.mjs",
    "archive:phase4": "scripts/operations/archive-phase4.sh",
    "supabase:migrate:web-cache": "node ./scripts/operations/apply-web-cache-migrations.mjs",
    "codegen:api": "python3 server/export_openapi.py && openapi-typescript openapi/fastapi.json -o packages/api-client/types.ts",
    "typecheck:web": "pnpm --filter web typecheck"
  },
