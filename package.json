{
  "name": "@prisma-glow/workspace",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=9.12.3"
  },
  "scripts": {
    "dev": "turbo dev --filter=!@prisma-glow/docs",
    "dev:web": "pnpm --filter @prisma-glow/web dev",
    "dev:client": "pnpm --filter @prisma-glow/client dev",
    "dev:admin": "pnpm --filter @prisma-glow/admin dev",
    "build": "turbo build",
    "build:web": "pnpm --filter @prisma-glow/web build",
    "build:netlify": "pnpm --filter @prisma-glow/web build",
    "postbuild:netlify": "echo 'Build output in apps/web/out'",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules .turbo dist",
    "db:generate": "node scripts/db-generate.mjs",
    "db:migrate": "node scripts/db-migrate.mjs",
    "db:seed": "tsx packages/database/seed.ts",
    "sync:knowledge-web-sources": "tsx scripts/syncKnowledgeWebSourcesFromYaml.ts",
    "ingest:accounting-kb": "tsx scripts/accounting-kb/ingest.ts",
    "analyze": "turbo run analyze",
    "lighthouse": "lighthouse https://prisma-glow.netlify.app --output html --view",
    "setup": "node scripts/setup.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.4.0",
    "@google/generative-ai": "^0.21.0",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@netlify/build": "^29.0.0",
    "@netlify/plugin-nextjs": "^5.0.0",
    "@types/dompurify": "^3.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.12.0",
    "dompurify": "^3.2.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lighthouse": "^12.2.1",
    "lint-staged": "^15.3.0",
    "netlify-plugin-cache-nextjs": "^1.0.0",
    "prettier": "^3.4.2",
    "supabase": "^2.54.11",
    "pg": "^8.11.5",
    "tsx": "^4.19.1",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.6.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/react-window": "^2.0.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.3",
    "tauri-plugin-store-api": "^0.0.0"
  }
}
