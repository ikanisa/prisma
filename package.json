{
  "name": "@prisma-glow/workspace",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=9.12.3"
  },
  "scripts": {
    "dev": "turbo dev --filter=!@prisma-glow/docs",
    "dev:web": "pnpm --filter @prisma-glow/web dev",
    "dev:client": "pnpm --filter @prisma-glow/client dev",
    "dev:admin": "pnpm --filter @prisma-glow/admin dev",
    "dev:tauri": "cd src-tauri && cargo tauri dev",
    "build": "turbo build",
    "build:web": "pnpm --filter @prisma-glow/web build",
    "build:tauri": "cd src-tauri && cargo tauri build",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "test:desktop": "playwright test --project=desktop-e2e",
    "test:desktop:unit": "vitest run tests/desktop/unit",
    "test:tauri": "cd src-tauri && cargo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules .turbo dist",
    "db:generate": "node scripts/db-generate.mjs",
    "db:migrate": "node scripts/db-migrate.mjs",
    "db:seed": "tsx packages/database/seed.ts",
    "sync:knowledge-web-sources": "tsx scripts/syncKnowledgeWebSourcesFromYaml.ts",
    "ingest:accounting-kb": "tsx scripts/accounting-kb/ingest.ts",
    "ingest:web": "tsx scripts/ingestKnowledgeFromWeb.ts",
    "analyze": "turbo run analyze",
    "lighthouse": "lighthouse https://prisma-glow.pages.dev --output html --view",
    "setup": "node scripts/setup.js",
    "dev:desktop-ui": "vite",
    "build:desktop-ui": "vite build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:macos": "TAURI_BUILD=true pnpm --filter @prisma-glow/web build && tauri build --target universal-apple-darwin",
    "desktop:dev": "tauri dev",
    "desktop:build": "TAURI_BUILD=true pnpm --filter @prisma-glow/web build && tauri build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.4.0",
    "@google/generative-ai": "^0.21.0",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tauri-apps/cli": "^2.0.0",
    "@types/dompurify": "^3.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.12.0",
    "@types/pdf-parse": "^1.1.4",
    "dompurify": "^3.2.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "js-sha256": "^0.11.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.2.1",
    "lint-staged": "^15.3.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.72.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.5",
    "prettier": "^3.4.2",
    "supabase": "^2.54.11",
    "tsx": "^4.19.1",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.6.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@types/react-window": "^2.0.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.3",
    "tauri-plugin-store-api": "^0.0.0"
  }
}
