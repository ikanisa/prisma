{
  "dashboard": {
    "title": "AI Agent Learning System",
    "tags": ["learning", "ai", "agents"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Feedback Collection Rate",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(learning_feedback_total[5m])",
            "legendFormat": "Feedback/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "User Satisfaction Rate",
        "type": "gauge",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "learning_satisfaction_rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.6, "color": "yellow" },
                { "value": 0.8, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Correction Rate",
        "type": "gauge",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "learning_correction_rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.2, "color": "yellow" },
                { "value": 0.3, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Training Examples by Type",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 8, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "learning_examples_total",
            "legendFormat": "{{example_type}}"
          }
        ]
      },
      {
        "id": 5,
        "title": "Background Job Queue Size",
        "type": "stat",
        "gridPos": { "x": 8, "y": 8, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "learning_queue_size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 100, "color": "yellow" },
                { "value": 500, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Training Runs (24h)",
        "type": "stat",
        "gridPos": { "x": 16, "y": 8, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "increase(learning_training_runs_total[24h])"
          }
        ]
      },
      {
        "id": 7,
        "title": "API Request Duration",
        "type": "graph",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(learning_api_request_duration_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(learning_api_request_duration_bucket[5m]))",
            "legendFormat": "p50"
          }
        ]
      },
      {
        "id": 8,
        "title": "Job Execution Time",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(learning_job_duration_sum[5m]) / rate(learning_job_duration_count[5m])",
            "legendFormat": "{{job_name}}"
          }
        ]
      },
      {
        "id": 9,
        "title": "Active A/B Experiments",
        "type": "stat",
        "gridPos": { "x": 0, "y": 24, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "learning_experiment_total{status=\"running\"}"
          }
        ]
      },
      {
        "id": 10,
        "title": "Dataset Quality Score",
        "type": "gauge",
        "gridPos": { "x": 6, "y": 24, "w": 9, "h": 4 },
        "targets": [
          {
            "expr": "learning_dataset_quality_score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.7, "color": "yellow" },
                { "value": 0.85, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "System Health",
        "type": "stat",
        "gridPos": { "x": 15, "y": 24, "w": 9, "h": 4 },
        "targets": [
          {
            "expr": "up{job=\"fastapi\"}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "1": { "text": "UP", "color": "green" },
                  "0": { "text": "DOWN", "color": "red" }
                }
              }
            ]
          }
        }
      }
    ]
  }
}
