{
  "name": "@prisma-glow/agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Agent registry and routing system for multi-engine AI agents",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./registry": "./src/registry/index.ts",
    "./openai": "./src/openai/index.ts",
    "./gemini": "./src/gemini/index.ts",
    "./router": "./src/router.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@prisma-glow/types": "workspace:*",
    "@prisma-glow/core": "workspace:*",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0"
  },
  "keywords": [
    "ai-agents",
    "agent-registry",
    "openai",
    "gemini",
    "tax",
    "audit",
    "accounting",
    "corporate-services"
  ]
}
