{
  "name": "@prisma-glow/agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Agent registry and routing system for multi-engine AI agents",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "import": "./dist/registry/index.js"
    },
    "./openai": {
      "types": "./dist/openai/index.d.ts",
      "import": "./dist/openai/index.js"
    },
    "./gemini": {
      "types": "./dist/gemini/index.d.ts",
      "import": "./dist/gemini/index.js"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "validate:registry": "node scripts/validate-registry.js",
    "validate:verbose": "node scripts/validate-registry.js --verbose",
    "validate:full": "node scripts/validate-registry.js --verbose --check-scopes --check-models"
  },
  "dependencies": {
    "@prisma-glow/core": "workspace:*",
    "@prisma-glow/types": "workspace:*",
    "js-yaml": "^4.1.0",
    "openai": "^4.104.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.49.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "eslint": "^8.57.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "ai-agents",
    "agent-registry",
    "openai",
    "gemini",
    "tax",
    "audit",
    "accounting",
    "corporate-services"
  ]
}
