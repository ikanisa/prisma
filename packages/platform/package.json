{
  "name": "@prisma-glow/platform",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "@prisma-glow/lib": "workspace:*"
  }
}
